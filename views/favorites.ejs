<% include ./partials/header.ejs %>
<div class="container">
  <% posts.forEach(post =>{ %>
  <div class="box">
    <div class="container_img">
      <a href="/showfull/?img=/images/uploads/<%= post.image %>&title=<%= post.title %>&desc=<%= post.description %>&username=<%= post.user.username %>" class="">
        <img src="/images/uploads/<%= post.image %>" alt="Your Image">
        <a href="/favourites/post/<%= post._id %>" class="overlay">
          <% if (user.favourites.indexOf(post._id) !== -1){ %>
            <i class="fa-solid fa-star i1"></i>
            <% } else { %>
            <i class="fa-regular fa-star i2"></i>
            <% } %>
        </a>
      </a>
    </div>
  </div>
  <% })%>
</div>
<% include ./partials/footer.ejs %>
